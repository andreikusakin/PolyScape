generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Preset {
    id          String   @id @default(cuid())
    author      String
    email       String
    presetName  String
    discription String?
    createdAt   DateTime @default(now())
    presetData  Json
}
