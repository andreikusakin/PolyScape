generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Preset {
    id         String   @id @default(cuid())
    name       String
    createdAt  DateTime @default(now())
    username   String
    presetData Json
}

model Test {
    id         String   @id @default(cuid())
    name       String
    createdAt  DateTime @default(now())
}